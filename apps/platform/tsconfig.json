{
  "extends": "ts-config/app.json",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
    }
  },
  "vueCompilerOptions": {
    "experimentalCompatMode": 2,
    "experimentalDisableTemplateSupport": true
  },
  "include": [
    "vite.config.*",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ]
}
