{
  "name": "core",
  "version": "0.0.1",
  "description": "配置化页面容器",
  "main": "./dist/core.umd.js",
  "module": "./dist/core.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "development": "./index.ts",
    "import": "./dist/core.es.js",
    "require": "./dist/core.umd.js"
  },
  "scripts": {
    "build": "vite build && npm run types",
    "typecheck": "vue-tsc --noEmit",
    "types": "vue-tsc --declaration --emitDeclarationOnly",
    "release": "release",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "devDependencies": {
    "renderer": "workspace:*",
    "tsconfig": "workspace:*",
    "scripts": "workspace:*"
  }
}
